# .env.example

# === MySQL (结构化主数据) ===
MYSQL_ROOT_PASSWORD=rootpassword
MYSQL_USER=smartquote
MYSQL_PASSWORD=smartpassword
MYSQL_HOST=mysql
MYSQL_PORT=3306
MYSQL_DATABASE=smartquote

# === PostgreSQL (向量数据) ===
POSTGRES_USER=smartquote
POSTGRES_PASSWORD=smartpassword
POSTGRES_HOST=postgres
POSTGRES_PORT=5432
POSTGRES_DB=smartquote_vector

# === Redis (缓存) ===
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0
REDIS_TTL_MATERIAL=3600      # 物料缓存 1h
REDIS_TTL_RATE=3600          # 费率缓存 1h
REDIS_TTL_LLM=86400          # LLM 缓存 24h

# === 阿里云 DashScope (通义千问) ===
DASHSCOPE_API_KEY=sk-xxx
DASHSCOPE_MODEL=qwen-plus
DASHSCOPE_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1

# === 前端 ===
NEXT_PUBLIC_API_URL=http://localhost:8000
